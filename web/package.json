{
  "name": "react-expressjs",
  "version": "1.0.0-beta",
  "description": "Isomorphic application written in ReactJS with ExpressJS server",
  "main": "server/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack/client.prod.js --progress",
    "dev": "webpack-dev-server --config webpack/client.dev.js",
    "dev:server": "npm run build && cross-env BABEL_ENV='node' nodemon --watch server server/index.js",
    "start": "npm run build && cross-env NODE_ENV=production node server/index.js",
    "lint": "eslint --ext .jsx,.js ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakhaa/react-express"
  },
  "keywords": [
    "react",
    "express",
    "webpack",
    "babel",
    "redux"
  ],
  "author": "Alex Bakh",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "apollo-boost": "^0.1.22",
    "apollo-cache-inmemory": "^1.3.11",
    "cross-env": "^5.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "formik": "^1.4.0",
    "graphql": "^14.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-apollo": "^2.3.2",
    "react-dom": "^16.6.3",
    "react-jss": "^8.6.1",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.2",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.8",
    "prettier": "1.15.3",
    "react-hot-loader": "^4.3.12",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
